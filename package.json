{
  "name": "health-dashboard",
  "version": "0.0.1",
  "description": "A dashboard for health data visualization.",
  "private": false,
  "main": "dist/index.js",
  "dependencies": {
    "@material-ui/core": "1.4.1",
    "@material-ui/icons": "2.0.0",
    "@types/googlemaps": "3.30.11",
